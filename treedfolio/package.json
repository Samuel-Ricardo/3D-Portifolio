{
  "name": "treedfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:clean": "npm run format:fix && npm run lint && npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:staged": "eslint **/*.{js,jsx,ts,tsx} --fix --cache",
    "format:verify": "prettier --check --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "format:fix": "prettier --write --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "husky:prepare": "cd ../ && husky install"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@react-three/drei": "^9.92.6",
    "@react-three/fiber": "^8.15.12",
    "framer-motion": "^10.16.16",
    "maath": "^0.10.7",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-tilt": "^1.0.2",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
